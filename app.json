{
  "pages": [
    "pages/index/index",
    "pages/logs/logs",
    "pages/scroll-view/scroll-view",
    "pages/report-list/report-list",
    "pages/login/login",
    "pages/test/test",
    "pages/test-scroll-click/test-scroll-click",
